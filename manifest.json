{
    "description": "DSDS Vorlage 1",
    "manifest_version": 2,
    "name": "dsds-template",
    "version": "1.0",

    "background": {
      "scripts": ["processor.js","capturer.js", "vizualizer.js"]
    },

    "permissions": [
      "activeTab",
      "<all_urls>",
      "webRequest",
      "unlimitedStorage",
      "tabs"
    ],

    "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-pixelperfect-32.png"
    }
  }
}
