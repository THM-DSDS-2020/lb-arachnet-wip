{
    "description": "Arachnet Version of lb",
    "homepage_url": "https://github.com/THM-DSDS-2020/lb-arachnet-wip",
    "manifest_version": 2,
    "name": "Arachnet",
    "version": "2.0",

    "background": {
      "scripts": ["processor.js","capturer.js", "vizualizer.js"]
    },

    "permissions": [
      "activeTab",
      "<all_urls>",
      "webRequest",
      "tabs",
      "storage"
    ],


    "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-pixelperfect-32.png"
    }
  },

    "options_ui": {
    "page": "options.html",
    "browser_style": true
    }
}
